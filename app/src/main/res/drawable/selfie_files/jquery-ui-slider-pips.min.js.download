	var r = $object_split_Array[0] + "_" + $object_split_Array[1];
	if (n) iconGroupButtonAction.SetValueByValueAttr($(t), n);
	var i = getIntValueFromInr($(t).val());
	if (i > 0) {
		var s = parseFloat(getIntValueFromInr($(t).val()));
		$("#"+r).slider("value", s)
	} else {
		$("#"+r).slider("value", $("#"+r).attr("min"))
	}
	var o = NewMoneyFormatInr($(t).val());
	if (o == 0) {
		o = ""
	}
	$(t).val(o)
}

