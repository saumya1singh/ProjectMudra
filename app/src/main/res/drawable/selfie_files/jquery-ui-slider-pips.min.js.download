function NewMoneyFormatInr(e) {
 var t = null;
 if (e.toString().indexOf(",") != "-1") {
  e = e.toString().replace(/,/g, "")
 }
 if (e > 0) {
  e = e.toString();
  var n = "";
  if (e.indexOf(".") > 0) {
   n = e.substring(e.indexOf("."), e.length)
  }
  e = Math.floor(e);
  e = e.toString();
  var r = e.substring(e.length - 3);
  var i = e.substring(0, e.length - 3);
  if (i != "") {
   r = "," + r
  }
  t = i.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + r + n
 } else {
  t = 0
 }
 return t
}

function getIntValueFromInr(e) {
 if (e != undefined) {
  if (e.toString().indexOf(",") != "-1") {
   return ("" + e).replace(/\,/g, "")
  } else {
   return e
  }
